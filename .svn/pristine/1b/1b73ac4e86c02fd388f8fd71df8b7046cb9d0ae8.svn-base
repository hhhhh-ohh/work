# ----------------------------------------

wm.mysql.db.master.username=root
wm.mysql.db.master.password=Wmi@2023
wm.mysql.db.master.url=172.19.25.118
wm.mysql.db.slave0.username=root
wm.mysql.db.slave0.password=Wmi@2023
wm.mysql.db.slave0.url=172.19.25.118
wm.mysql.db.name=sbc-setting

