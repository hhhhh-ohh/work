                                                 `lar_id_card` varchar(18) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '法人身份证号',
                                                 `lar_id_card_st_dt` date DEFAULT NULL COMMENT '身份证开始日期',
                                                 `lar_id_card_exp_dt` date DEFAULT NULL COMMENT '身份证有效期结束时间',
                                                 `mer_contact_name` varchar(50) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '商户联系人',
                                                 `mer_contact_mobile` varchar(20) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '商户联系人手机号',
                                                 `acct_name` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '账户名称',
                                                 `acct_no` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '账户卡号',
                                                 `acct_certificate_no` varchar(18) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '账户证件号',
                                                 `openning_bank_code` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '账户开户行号',
                                                 `openning_bank_name` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '账户开户行名称',
                                                 `clearing_bank_code` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '账户清算行号',
                                                 `id_card_front_pic` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '法人身份证正面',
                                                 `id_card_back_pic` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '法人身份证背面',
                                                 `bank_card_pic` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '银行卡',
                                                 `business_pic` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '营业执照',
                                                 `merchant_pic` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '商户门头照',
                                                 `shopinner_pic` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '商户内部照',
                                                 `account_state` tinyint(4) DEFAULT NULL COMMENT '开户审核状态 0、未进件 1、审核中 2、审核成功 、3、审核失败',
                                                 `ledger_state` tinyint(4) DEFAULT NULL COMMENT '商户分账审核状态 0、未开通 1、审核中 2、审核成功 3、审核失败',
                                                 `account_reject_reason` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '开户驳回原因',
                                                 `ledger_reject_reason` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '分账驳回原因',
                                                 `account_type` tinyint(4) DEFAULT NULL COMMENT '账户类型 0、商户 1、接收方',
                                                 `receiver_type` tinyint(4) DEFAULT NULL COMMENT '接收方类型 0、平台 1、供应商 2、分销员',
                                                 `contract_id` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '进件id',
                                                 `ec_apply_id` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '电子合同受理号',
                                                 `ec_url` text COLLATE utf8mb4_unicode_ci COMMENT '待签约的电子合同链接',
                                                 `ec_content` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '电子合同文件',
                                                 `create_time` datetime DEFAULT NULL COMMENT '创建时间',
                                                 `create_person` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '创建人',
                                                 `update_time` datetime DEFAULT NULL COMMENT '修改时间',
                                                 `update_person` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '修改人',
                                                 `del_flag` tinyint(4) DEFAULT NULL COMMENT '是否删除 0、未删除 1、已删除',
                                                 `term_no` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '终端号',
                                                 `mer_cup_no` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '银联商户号',
                                                 `e_mail` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '邮箱',
                                                 `ledger_apply_id` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '分账申请编号',
                                                 PRIMARY KEY (`id`),
                                                 KEY `idx_business_id` (`business_id`) USING BTREE,
                                                 KEY `idx_third_mem_no` (`third_mem_no`) USING BTREE,
                                                 KEY `idx_term_no` (`term_no`) USING BTREE,
                                                 KEY `idx_mer_cup_no` (`mer_cup_no`) USING BTREE,
                                                 KEY `idx_contract_id` (`contract_id`) USING BTREE,
                                                 KEY `idx_ec_apply_id` (`ec_apply_id`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci COMMENT='清分账户';

-- 分账合同协议配置
DROP TABLE IF EXISTS `sbc-customer`.`ledger_contract`;
CREATE TABLE `sbc-customer`.`ledger_contract` (
                                                  `id` bigint(11) NOT NULL,
                                                  `title` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '协议标题',
                                                  `content` text COLLATE utf8mb4_unicode_ci COMMENT '协议内容',
                                                  `body` text COLLATE utf8mb4_unicode_ci COMMENT '正文',
                                                  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci COMMENT='分账合同协议配置';

INSERT INTO `sbc-customer`.`ledger_contract`(`id`, `title`, `content`, `body`) VALUES (0, '分账合作协议', '<p style=\";text-align: center;font-family: Calibri;font-size: 14px;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><strong><span style=\"font-family: 宋体;line-height: 32px;font-size: 21px\">分账合作协议</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">&nbsp;</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">甲方：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">${partA}</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙方：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">${partB}</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;text-indent: 32px;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">为了更好地开拓市场，更好地为本地客户服务，甲乙双方本着互惠互利的双赢策略，根据乙</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">方</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">入驻条件，甲方申请入驻乙方商城，并完全接受乙方的规范，经友好协商，甲乙双方签署以下入驻协议。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">一、</span>&nbsp;入驻商家基本条件</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、有良好的合作意愿，能提供优质的商品，保证合作的顺利进行，并保证提供良好的售后服务；</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、甲方必须满足以下条件之一（符合其中一项即可）：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑴</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">授权商，获得国际或者国内知名品牌厂商的授权；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑵</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">拥有自己注册商标的生产型厂商；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑶</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">专业品牌经销商，代理商；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑷</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">专业品牌专卖店。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、甲方应在签订本协议时向乙方提供（经乙方认可的）包括但不限于以下证明材料复印件：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑴</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">营业执照</span> (包括个体户营业执照)、税务登记证；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑵</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">拥有注册商标或者品牌，或者拥有正规的品牌授权书。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、甲方承诺：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑴</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">所有商品一口价销售（参与打折、促销、秒杀活动除外）；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑵</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">七天无损坏不影响二次销售情况下换货</span>(如商品无质量问题，且包装未破坏不影响二次销售，换货费用由买家承担，每张订单只提供一次换货服务；如属商品质量问题，换货费用由商家承担)；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑶</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">实体店地址变更应及时告知乙方；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑷</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">参加乙方全网积分购物活动；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑸</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">所有商品保证原厂正品，保障商品质量、承诺售后服务，必要时能提供销售发票。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">二</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;商铺入驻开通流程</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、&nbsp;甲乙双方签署本协议；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、&nbsp;甲方根据所选择商铺类型，提交相应的证明材料复印件；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、&nbsp;在乙方商城注册，获取管理员账号；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、&nbsp;乙方审核通过后，甲方即可发布商品并展示经营活动。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">三</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;双方权利义务</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">甲方权利义务：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、在本协议第一条规定范围内，甲方自行开拓市场与发展业务，不得以欺诈、胁迫等不正当手段损害客户或</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">方的利益与声誉；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、甲方妥善保管商铺管理员账号，如因甲方保管、设置或使用不当造成的经济损失，由甲方自行承担责任；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、甲方在使用过程中如发现任何非法盗用乙方账号出现安全漏洞等情况，应立即通知乙方；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、甲方保证其所有经营活动完全符合中国有关法律、法规、行政规章等的规定。如因甲方违反上述规定给乙方带来任何损害，甲方应承担所有法律责任并赔偿由此给</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">方造成的损失；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">5、在协议有效期内，甲方不得向与乙方构成商业竞争关系的企业、商业机构或者组织提供有关乙方相关信息或者资料，否则对乙方造成损失的，由甲方负责赔偿。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">6、甲方在乙方平台销售商品的，乙方按照</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">不高于</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">销售金额的</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">90</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">%向甲方收取服务费。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙方权利义务：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">、</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙方对甲方提供必要的开店技术咨询，并保证商城系统平台能正常稳定运行；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">、</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">如甲方是托管商铺，乙方应及时为甲方进行店铺更新并实时进行日常管理；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、在本协议有效期限内，乙方有权根据市场情况对各种商铺入驻条件进行调整。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">四</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;协议期限</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">本协议有效期为一年，自签订之日起生效。有效期满后，双方可视合作情况续约。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">&nbsp;</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">五</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;协议变更、终止及违约责任</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、甲乙双方应本着诚实信用的原则履行本协议。任何一方在履约过程中采用欺诈、胁迫或者暴力的手段，另一方均可以解除本协议并要求对方赔偿由此造成的损失；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、在协议执行期间，如果双方或一方认为需要终止，应提前一个月通知对方，双方在财务结算完毕、各自责任明确履行之后，方可终止协议。某方擅自终止本协议，给对方造成损失的，应赔偿对方损失；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、经双方协商达成一致，可以对本协议有关条款进行变更，但应当以书面形式确认；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、一方变更通讯地址或其它联系方式，应自变更之日起十日内，将变更后的地址、联系方式通知另一方，否则变更方应对此造成的一切后果承担责任；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">5、如因客观情况发生重大变化，致使本协议无继续法履行的，经甲乙双方协商同意，可以变更或者终止协议的履行。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">六</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;保密条款</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、甲、乙双方所提供给对方的一切资料、技术和对项目的策划设计要严格保密，并只能在合作双方公司的业务范围内使用；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、甲、乙双方均应对在合作过程中所知悉的对方的商业和技术秘密承担保密义务。保密条款不受本协议期限的限制。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">七</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;争议解决</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">在本协议执行期间如果双方发生争议，双方应友好协商解决。如果协商不成，双方同意提交给仲裁</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">机构</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">进行仲裁，接受其仲裁规则，并服从该仲裁裁决，仲裁费由败诉方承担。争议的解决适用中华人民共和国法律、法规、条例和计算机行业惯例。</span></p><p style=\"padding-left: 10px; padding-right: 10px;overflow: hidden; font-size:14px;\"><br/></p>', '<p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;text-indent: 32px;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">为了更好地开拓市场，更好地为本地客户服务，甲乙双方本着互惠互利的双赢策略，根据乙</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">方</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">入驻条件，甲方申请入驻乙方商城，并完全接受乙方的规范，经友好协商，甲乙双方签署以下入驻协议。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">一、</span>&nbsp;入驻商家基本条件</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、有良好的合作意愿，能提供优质的商品，保证合作的顺利进行，并保证提供良好的售后服务；</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、甲方必须满足以下条件之一（符合其中一项即可）：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑴</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">授权商，获得国际或者国内知名品牌厂商的授权；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑵</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">拥有自己注册商标的生产型厂商；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑶</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">专业品牌经销商，代理商；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑷</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">专业品牌专卖店。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、甲方应在签订本协议时向乙方提供（经乙方认可的）包括但不限于以下证明材料复印件：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑴</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">营业执照</span> (包括个体户营业执照)、税务登记证；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑵</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">拥有注册商标或者品牌，或者拥有正规的品牌授权书。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、甲方承诺：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑴</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">所有商品一口价销售（参与打折、促销、秒杀活动除外）；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑵</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">七天无损坏不影响二次销售情况下换货</span>(如商品无质量问题，且包装未破坏不影响二次销售，换货费用由买家承担，每张订单只提供一次换货服务；如属商品质量问题，换货费用由商家承担)；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑶</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">实体店地址变更应及时告知乙方；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑷</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">参加乙方全网积分购物活动；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">⑸</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">所有商品保证原厂正品，保障商品质量、承诺售后服务，必要时能提供销售发票。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">二</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;商铺入驻开通流程</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、&nbsp;甲乙双方签署本协议；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、&nbsp;甲方根据所选择商铺类型，提交相应的证明材料复印件；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、&nbsp;在乙方商城注册，获取管理员账号；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、&nbsp;乙方审核通过后，甲方即可发布商品并展示经营活动。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">三</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;双方权利义务</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">甲方权利义务：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、在本协议第一条规定范围内，甲方自行开拓市场与发展业务，不得以欺诈、胁迫等不正当手段损害客户或</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">方的利益与声誉；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、甲方妥善保管商铺管理员账号，如因甲方保管、设置或使用不当造成的经济损失，由甲方自行承担责任；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、甲方在使用过程中如发现任何非法盗用乙方账号出现安全漏洞等情况，应立即通知乙方；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、甲方保证其所有经营活动完全符合中国有关法律、法规、行政规章等的规定。如因甲方违反上述规定给乙方带来任何损害，甲方应承担所有法律责任并赔偿由此给</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">方造成的损失；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">5、在协议有效期内，甲方不得向与乙方构成商业竞争关系的企业、商业机构或者组织提供有关乙方相关信息或者资料，否则对乙方造成损失的，由甲方负责赔偿。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">6、甲方在乙方平台销售商品的，乙方按照</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">不高于</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">销售金额的</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">90</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">%向甲方收取服务费。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙方权利义务：</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">、</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">乙方对甲方提供必要的开店技术咨询，并保证商城系统平台能正常稳定运行；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">、</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">如甲方是托管商铺，乙方应及时为甲方进行店铺更新并实时进行日常管理；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、在本协议有效期限内，乙方有权根据市场情况对各种商铺入驻条件进行调整。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">四</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;协议期限</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">本协议有效期为一年，自签订之日起生效。有效期满后，双方可视合作情况续约。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">&nbsp;</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">五</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;协议变更、终止及违约责任</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、甲乙双方应本着诚实信用的原则履行本协议。任何一方在履约过程中采用欺诈、胁迫或者暴力的手段，另一方均可以解除本协议并要求对方赔偿由此造成的损失；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、在协议执行期间，如果双方或一方认为需要终止，应提前一个月通知对方，双方在财务结算完毕、各自责任明确履行之后，方可终止协议。某方擅自终止本协议，给对方造成损失的，应赔偿对方损失；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">3、经双方协商达成一致，可以对本协议有关条款进行变更，但应当以书面形式确认；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">4、一方变更通讯地址或其它联系方式，应自变更之日起十日内，将变更后的地址、联系方式通知另一方，否则变更方应对此造成的一切后果承担责任；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">5、如因客观情况发生重大变化，致使本协议无继续法履行的，经甲乙双方协商同意，可以变更或者终止协议的履行。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">六</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;保密条款</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">1、甲、乙双方所提供给对方的一切资料、技术和对项目的策划设计要严格保密，并只能在合作双方公司的业务范围内使用；</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">2、甲、乙双方均应对在合作过程中所知悉的对方的商业和技术秘密承担保密义务。保密条款不受本协议期限的限制。</span></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">七</span></strong><strong><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><span style=\"font-family:宋体\">、</span>&nbsp;争议解决</span></strong></p><p style=\";text-align: justify;font-family: Calibri;font-size: 14px;white-space: normal;line-height: 21px padding-left: 10px; padding-right: 10px; overflow: hidden;\"><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\"><br/></span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">在本协议执行期间如果双方发生争议，双方应友好协商解决。如果协商不成，双方同意提交给仲裁</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">机构</span><span style=\"font-family: 宋体;line-height: 24px;font-size: 16px\">进行仲裁，接受其仲裁规则，并服从该仲裁裁决，仲裁费由败诉方承担。争议的解决适用中华人民共和国法律、法规、条例和计算机行业惯例。</span></p><p style=\"padding-left: 10px; padding-right: 10px;overflow: hidden; font-size:14px;\"><br/></p>');


-- 分账接口错误记录
DROP TABLE IF EXISTS `sbc-customer`.`ledger_error_record`;
CREATE TABLE `sbc-customer`.`ledger_error_record` (
                                                      `id` varchar(32) COLLATE utf8mb4_unicode_ci NOT NULL,
                                                      `type` tinyint(4) DEFAULT NULL COMMENT '业务类型 0、创建账户',
                                                      `business_id` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '业务id',
                                                      `retry_count` int(10) DEFAULT NULL COMMENT '重试次数',
                                                      `state` tinyint(4) DEFAULT NULL COMMENT '处理状态 0、待处理 1、处理中 2、处理成功 3、处理失败',
                                                      `error_info` text COLLATE utf8mb4_unicode_ci COMMENT '错误信息',
                                                      `create_time` datetime DEFAULT NULL COMMENT '创建时间',
                                                      `create_person` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '创建人',
                                                      `update_time` datetime DEFAULT NULL COMMENT '修改时间',
                                                      `update_person` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '修改人',
                                                      PRIMARY KEY (`id`),
                                                      KEY `idx_ business_id` (`business_id`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci COMMENT='分账接口错误记录';

-- 分账文件
DROP TABLE IF EXISTS `sbc-customer`.`ledger_file`;
CREATE TABLE `sbc-customer`.`ledger_file` (
                                              `id` varchar(32) COLLATE utf8mb4_unicode_ci NOT NULL,
                                              `content` mediumblob COMMENT '文件内容',
                                              `file_ext` varchar(20) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '文件扩展名',
                                              `content_str` mediumtext COLLATE utf8mb4_unicode_ci COMMENT '合同内容，html字符串',
                                              PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci COMMENT='分账文件';

-- 分账绑定关系
DROP TABLE IF EXISTS `sbc-customer`.`ledger_receiver_rel`;
CREATE TABLE `sbc-customer`.`ledger_receiver_rel` (
                                                      `id` varchar(32) COLLATE utf8mb4_unicode_ci NOT NULL,
                                                      `ledger_supplier_id` varchar(32) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT '商户分账数据id',
                                                      `supplier_id` bigint(11) NOT NULL COMMENT '商户id',
                                                      `receiver_id` varchar(50) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT '接收方id',
                                                      `receiver_name` varchar(50) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '接收方名称',
                                                      `receiver_code` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '接收方编码(供应商需要)',
                                                      `receiver_type` tinyint(4) DEFAULT NULL COMMENT '接收方类型 0、平台 1、供应商 2、分销员',
                                                      `account_state` tinyint(4) DEFAULT NULL COMMENT '开户审核状态 0、未进件 1、审核中 2、审核成功 3、审核失败',
                                                      `bind_state` tinyint(4) DEFAULT NULL COMMENT '绑定状态 0、未绑定 1、审核中 2、已绑定 3、绑定失败',
                                                      `bind_time` datetime DEFAULT NULL COMMENT '绑定时间',
                                                      `apply_id` varchar(128) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '外部绑定受理编号',
                                                      `del_flag` tinyint(4) DEFAULT NULL COMMENT '删除标识 0、未删除 1、已删除',
                                                      PRIMARY KEY (`id`),
                                                      KEY `idx_ledger_supplier_id` (`ledger_supplier_id`) USING BTREE,
                                                      KEY `idx_supplier_id` (`supplier_id`) USING BTREE,
                                                      KEY `idx_receiver_id` (`receiver_id`) USING BTREE,
                                                      KEY `idx_apply_id` (`apply_id`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci COMMENT='分账绑定关系';


-- 分账绑定关系补偿记录表
DROP TABLE IF EXISTS `sbc-customer`.`ledger_receiver_rel_record`;
CREATE TABLE `sbc-customer`.`ledger_receiver_rel_record` (
                                                             `id` varchar(32) COLLATE utf8mb4_unicode_ci NOT NULL,
                                                             `account_id` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '清分账户id',
                                                             `business_type` tinyint(4) DEFAULT NULL COMMENT '账户类型 0、商户 1、接收方',
                                                             `create_time` datetime DEFAULT NULL COMMENT '创建时间',
                                                             `create_person` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '创建人',
                                                             `update_time` datetime DEFAULT NULL COMMENT '修改时间',
                                                             `update_person` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '修改人',
                                                             PRIMARY KEY (`id`),
                                                             KEY `idx_ business_id` (`account_id`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci COMMENT='分账绑定关系补偿记录表';

-- 商户分账绑定数据表
DROP TABLE IF EXISTS `sbc-customer`.`ledger_supplier`;
CREATE TABLE `sbc-customer`.`ledger_supplier` (
                                                  `id` varchar(32) COLLATE utf8mb4_unicode_ci NOT NULL,
                                                  `ledger_account_id` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '清分账户id',
                                                  `company_info_id` bigint(11) DEFAULT NULL COMMENT '商家id',
                                                  `company_name` varchar(50) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '商家名称',
                                                  `company_code` varchar(32) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT '商家编号',
                                                  `plat_bind_state` tinyint(4) DEFAULT NULL COMMENT '平台绑定状态 0、未绑定 1、已绑定',
                                                  `provider_num` bigint(11) DEFAULT NULL COMMENT '供应商绑定数',
                                                  `distribution_num` bigint(11) DEFAULT NULL COMMENT '分销员绑定数',
                                                  PRIMARY KEY (`id`),
                                                  KEY `idx_company_info_id` (`company_info_id`) USING BTREE
