DROP TABLE IF EXISTS `sbc-account`.lakala_settlement;
create table `sbc-account`.lakala_settlement
(
    settle_id                     int auto_increment comment '主键' primary key,
    settle_uuid                   varchar(32)                 null comment 'mongoDB外键',
    create_time                   datetime                    null comment '结算单创建时间',
    start_time                    varchar(32)                 null comment '结算单开始时间',
    end_time                      varchar(32)                 null comment '结算单结束时间',
    store_id                      bigint                      null comment '店铺Id',
    total_amount                  decimal(20, 2)              null comment '分账总金额',
    total_provider_price          decimal(20, 2) default 0.00 null comment '供应商分账总额 (供货额+运费)',
    total_store_price             decimal(20, 6) default 0.00 null comment '店铺分账总额',
    total_commission_price        decimal(20, 2) default 0.00 null comment '分销佣金分账总额',
    total_platform_price          decimal(20, 6) default 0.00 null comment '平台佣金分账总额',
    total_split_pay_price         decimal(20, 2) default 0.00 null comment '商品实付总额',
    total_common_coupon_price     decimal(20, 2) default 0.00 null comment '通用券优惠总额',
    total_points                  bigint                      null comment '积分数量',
    point_price                   decimal(20, 2) default 0.00 null comment '积分抵扣总额',
    provider_goods_total_price    decimal(20, 2) default 0.00 null comment '供应商供货总额',
    provider_delivery_total_price decimal(20, 2) default 0.00 null comment '供货运费',
    delivery_price                decimal(20, 2) default 0.00 null comment '总运费(包含供应商运费)',
    lakala_ledger_status          tinyint                     null comment '结算状态(0:未结算,1:分账中,2:分账成功,3:分账失败,4:分账部分成功)'


)
    comment '拉卡拉分账结算单';

create index idx_store_id
    on `sbc-account`.lakala_settlement (store_id);

